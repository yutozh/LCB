{
  "name": "ebookcoin",
  "version": "0.1.3",
  "privated": true,
  "scripts": {
    "start": "node app.js",
    "test": "mocha",
    "cov": "mocha --require blanket -R html-cov test/helpers test/logic > tmp/coverage.html"
  },
  "engines": {
    "node": "0.12.*"
  },
  "author": "",
  "dependencies": {
    "array.prototype.find": "~1.0.0",
